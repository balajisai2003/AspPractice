@model AspPractice.Models.Domain.Emply
@{
}

<h1>Edit Employee</h1>
<div>
    <form method="post">
        <div class="form-group">
            <label>Id</label>
            <input type="text" class="form-control" asp-for="Id" readonly/>
            <label>Name</label>
            <input type="text" class="form-control" asp-for="Name" />
            <label>DeptId</label>
            <input type="text" class="form-control" asp-for="DeptId" />
            <label>UserName</label>
            <input type="text" class="form-control" asp-for="UserName" />
            <label>Password</label>
            <input type="text" class="form-control" asp-for="Password" />
        </div>
        <button type="submit" class="btn btn-primary" asp-area="" asp-controller="Emply" asp-action="Edit">Update</button>
        <button type="submit" class="btn btn-danger" asp-area="" asp-controller="Emply" asp-action="Delete">Delete</button>
    </form>
</div>